{
  "name": "wisdowm-journey-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "git@github.com:ephiscience/wisdowm-journey-server.git",
  "private": true,
  "dependencies": {
    "apollo-server": "^2.19.0",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pg": "^8.4.1",
    "ts-node": "^9.0.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "load_questions": "node --unhandled-rejections=strict -r dotenv/config -r ts-node/register ./src/scripts/load_questions.ts",
    "hello": "node --unhandled-rejections=strict -r dotenv/config -r ts-node/register ./src/scripts/hello.ts",
    "start": "node --unhandled-rejections=strict -r dotenv/config -r ts-node/register ./src/index.ts",
    "serve": "node --unhandled-rejections=strict -r dotenv/config -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      ".env"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/pg": "^7.14.5",
    "nodemon": "^2.0.6"
  }
}

{
  "name": "wisdowm-journey-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "git@github.com:ephiscience/wisdowm-journey-server.git",
  "private": true,
  "dependencies": {
    "@prisma/client": "^2.23.0",
    "apollo-server": "^2.24.1",
    "csv-parser": "^3.0.0",
    "graphql": "^15.5.0",
    "nexus": "^1.0.0"
  },
  "scripts": {
    "load_questions": "node --unhandled-rejections=strict -r ts-node/register ./src/scripts/load_questions.ts",
    "load_criterions": "node --unhandled-rejections=strict -r ts-node/register ./src/scripts/load_criterions.ts",
    "dev": "ts-node-dev --transpile-only --no-notify src/index.ts",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src",
      ".env"
    ],
    "exec": "yarn start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/node": "^14.14",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.3.0",
    "prisma": "^2.23.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
